{
  "name": "Papio",
  "version": "0.1",
  "description": "Quick mail to yourself",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon_128.png"
  },
  "icons" : {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
